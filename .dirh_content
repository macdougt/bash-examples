# dirhistory

function dirHistoryFunction() {
   if [ -z "$1" ]
   then
      list ~/.dirhistory
   else
      list ~/.dirhistory | grep $*
   fi
}

alias dh=dirHistoryFunction
alias sdh='dh | grep $@'
alias cdl='cd `tail -2 ~/.dirhistory | head -1`'

# Override cd - change directory
function cd() { builtin cd "$@" && appendif `pwd`;}
export -f cd
function cn() { cd `cdn $1`; }
export -f cn
function cdd() { cd `getDir $1`; }
export -f cdd
function cdf() { cd `cd_to_file $1`; }
export -f cdf

