# dirhistory
alias dh='list ~/.dirhistory'
alias sdh='dh | grep $@'
alias cdl='cd `tail -2 ~/.dirhistory | head -1`'
# Override cd - change directory
function cd() { builtin cd "$@" && appendif `pwd`;}
export -f cd
function cn() { cd `cdn $1`; }
export -f cn
function cdd() { cd `getDir $1`; }
export -f cdd
